{
  "expo": {
    "name": "ClassAway",
    "slug": "ClassAway",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sihanwee.ClassAway"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon.png",
        "backgroundColor": "#6559ff"
      },
      "package": "com.sihanwee.ClassAway"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
